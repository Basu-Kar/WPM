<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.org.employeevo.EmployeeVo"  table="emp">
    <id name="empId" column="emp_Id" type="int">
     <generator class="native"></generator>  
    </id>
    <property name="empFname" column="emp_fname"></property>  
    <property name="lastName" column="emp_lname"></property>  
    <property name="empEmailid" column="emp_email"></property>  
    <property name="salary" column="emp_sal"></property>
  
  </class> 
  
  
  
            
 </hibernate-mapping>