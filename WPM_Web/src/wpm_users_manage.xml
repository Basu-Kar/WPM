<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping package="">  
  <class name="com.ksoft.wpm.login.vo.LoginVO"  table="wpm" entity-name="authVO">
    <id name="userId" column="email" type=""></id>
    <property name="password" column="password"></property>
  
  </class> 
  
  <class name="com.ksoft.wpm.common.vo.UserVO"  table="wpm" entity-name="userVO">
    <id name="userId" column="user_id" type=""></id>
    <property name="fanme" column="f_name"></property>
    <property name="lname" column="l_name"></property>
    <property name="password" column="role_id"></property>
    <property name="emailId" column="email"></property>
    
  
  </class> 
  
  
            
 </hibernate-mapping>